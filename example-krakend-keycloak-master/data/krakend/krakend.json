{
    "version": 3,
    "extra_config": {
        "github_com/devopsfaith/krakend-gologging": {
            "level": "DEBUG",
            "prefix": "[KRAKEND]",
            "syslog": false,
            "stdout": true,
            "format": "default"
        },
        "security/cors": {
            "allow_origins": [
                "*"
            ],
            "allow_methods": [
                "OPTIONS",
                "GET",
                "HEAD",
                "POST"
            ],
            "expose_headers": [
                "Content-Length",
                "Content-Type"
            ],
            "allow_headers": [
                "Accept-Language"
            ],
            "max_age": "12h",
            "allow_credentials": false,
            "debug": true
        }

    },
    "timeout": "10000ms",
    "cache_ttl": "300s",
    "output_encoding": "json",
    "name": "Test Service",
    "endpoints": [
        {
            "endpoint": "/pd/products",
            "method": "GET",

            "extra_config": {
                "auth/validator": {
                    "alg": "RS256",
                    "jwk_url": "http://keycloak:8080/auth/realms/testRealm/protocol/openid-connect/certs",
                    "operation_debug": true,
                    "disable_jwk_security": true
                }
            },
            "output_encoding": "json",
            "concurrent_calls": 1,
            "backend": [
                {
                    "url_pattern": "/pd/products",
                    "encoding": "json",
                    "sd": "static",
                    "extra_config": {},
                    "host": [
                        "http://finalWebshop:8080"
                    ],
                    "is_collection": true,
                    "mapping": {
                        "collection": "products"
                    }
                }
            ]
        },
        {
            "endpoint": "/mock/bogus-new-api/{path}",
            "method": "GET",
            "extra_config": {
                "github.com/devopsfaith/krakend-jose/validator": {
                    "alg": "RS256",
                    "jwk_url": "http://host.docker.internal:8403/auth/realms/master/protocol/openid-connect/certs",
                    "issuer": "http://localhost:8403/auth/realms/master",
                    "disable_jwk_security": true
                },
                "github.com/devopsfaith/krakend/proxy": {
                    "static": {
                        "data": {
                            "new_field_a": 123,
                            "new_field_b": [
                                "arr1",
                                "arr2"
                            ],
                            "new_field_c": {
                                "obj": "obj1"
                            }
                        },
                        "strategy": "always"
                    }
                }
            },
            "output_encoding": "json",
            "concurrent_calls": 1,
            "backend": [
                {
                    "url_pattern": "/not-finished-yet",
                    "encoding": "json",
                    "sd": "static",
                    "extra_config": {},
                    "host": [
                        "nothing-here"
                    ],
                    "disable_host_sanitize": false
                }
            ]
        }
    ]
}
